{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:ci": "vite build",
    "preview": "vite preview",
    "lint": "eslint --max-warnings=0 .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run --reporter=basic",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "admin:swarm:verify": "node tests/scripts/verify-router.mjs && playwright test tests/admin.swarm.spec.ts",
    "verify:mvp": "npm run test:run",
    "ci:verify": "npm run lint && npm run test:run && npm run build:ci && node tools/findPattern.mjs \"TopBar\" && node tools/findPattern.mjs \"Tabs\\W\" && node tools/findPattern.mjs \"<svg\\s\"",
    "ci:verify:soft": "npm run lint && npm run test:run && npm run build:ci && (node tools/findPattern.mjs \"TopBar\" || true) && (node tools/findPattern.mjs \"Tabs\\W\" || true) && (node tools/findPattern.mjs \"<svg\\s\" || true)",
    "download-usda": "node scripts/download-usda.ts",
    "download-cnf": "node scripts/download-cnf.ts",
    "import-usda": "tsx scripts/import-usda.ts",
    "import-cnf": "tsx scripts/import-cnf.ts",
    "export:prompts": "tsx scripts/export_personality_prompts.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.344.0",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.30.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.0.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
